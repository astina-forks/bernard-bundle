{
    "name": "bernard/bernard-bundle",
    "type": "symfony-bundle",
    "description": "Symfony BernardBundle",
    "keywords": ["Queue processing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Cordova",
            "email": "cordoval@gmail.com"
        },
        {
            "name": "Stan Lemon",
            "email": "stosh1985@gmail.com"
        },
        {
            "name": "Bernard Community",
            "homepage": "https://github.com/bernard/BernardBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "bernard/bernard": "0.11.*@dev"
    },
    "require-dev": {
        "iron-io/iron_mq": "~1.4",
        "aws/aws-sdk-php": "~2.4",
        "predis/predis": "~0.8",
        "doctrine/dbal": "~2.1"
