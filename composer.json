{
    "name": "cordoval/bernard-bundle",
    "type": "symfony-bundle",
    "description": "Symfony BernardBundle",
    "keywords": ["Queue processing"],
    "homepage": "http://craftitonline.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Cordova",
            "email": "cordoval@gmail.com"
        },
        {
            "name": "Bernard Community",
            "homepage": "https://github.com/cordoval/CordovalBernardBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "bernard/bernard": "0.11.*@dev",
        "iron-io/iron_mq": "~1.4",
        "aws/aws-sdk-php" : "~2.4",
        "predis/predis" : "~0.8"
    },
    "suggest": {
        "predis/predis" : "only if redis enabled"
    },
    "autoload": {
        "psr-0": { "Cordoval\\BernardBundle": "" }
    },
    "target-dir": "Cordoval/BernardBundle"
}